{
  "metadataSchema": {
    "type": "object",
    "properties": {
      "creators": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "title": "Name",
              "minLength": 1,
              "$dataciteRequired": true
            },
            "nameType": {
              "type": "string",
              "title": "Name Type",
              "$dataciteRequired": false,
              "enum": [ "Personal", "Organizational" ],
              "pattern": "Personal",
              "default": "Personal"
            },
            "givenName": {
              "type": "string",
              "title": "Given Name",
              "minLength": 1,
              "$dataciteRequired": false
            },
            "familyName": {
              "type": "string",
              "title": "Family Name",
              "minLength": 1,
              "$dataciteRequired": false
            },
            "nameIdentifiers": {
              "type": "array",
              "title": "Name Identifiers",
              "items": {
                "type": "object",
                "properties": {
                  "nameIdentifier": {
                    "type": "string",
                    "title": "ORCID",
                    "pattern": "^https://orcid.org/(\\d{4}-){3}\\d{3}(\\d|X)$",
                    "$dataciteRequired": false
                  },
                  "nameIdentifierScheme": {
                    "type": "string",
                    "title": "Identifier Scheme",
                    "enum": [ "ORCID" ],
                    "default": "ORCID",
                    "$dataciteRequired": true
                  },
                  "schemeUri": {
                    "type": "string",
                    "title": "Scheme URI",
                    "enum": [ "https://orcid.org" ],
                    "default": "https://orcid.org",
                    "$dataciteRequired": false
                  }
                },
                "required": [ "nameIdentifier", "nameIdentifierScheme", "schemeUri" ]
              },
              "$dataciteRequired": false
            },
            "affiliation": {
              "type": "array",
              "title": "Affiliations*",
              "minItems": 1,
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "title": "Affiliation Name",
                    "minLength": 1,
                    "$dataciteRequired": true
                  },
                  "affiliationIdentifier": {
                    "type": "string",
                    "pattern": "^https://ror.org/0[a-z|0-9]{6}[0-9]{2}$",
                    "title": "ROR URL",
                    "$dataciteRequired": false
                  },
                  "affiliationIdentifierScheme": {
                    "type": "string",
                    "title": "Affiliation Identifier Scheme",
                    "enum": [ "ROR" ],
                    "$dataciteRequired": true
                  },
                  "schemeUri": {
                    "type": "string",
                    "title": "Scheme URI",
                    "enum": [ "https://ror.org" ],
                    "$dataciteRequired": false
                  }
                },
                "required": [ "name" ],
                "dependentRequired": {
                  "affiliationIdentifier": [ "affiliationIdentifierScheme", "schemeUri" ]
                }
              },
              "$dataciteRequired": false
            }
          },
          "required": [ "name", "givenName", "familyName", "affiliation" ]
        },
        "$dataciteRequired": true
      },
      "publisher": {
        "type:": "object",
        "title": "Publisher",
        "properties": {
          "name": {
            "type": "string",
            "title": "Publisher Name",
            "default": "PSI Open Data Provider",
            "$dataciteRequired": true
          },
          "publisherIdentifier": {
            "type": "string",
            "title": "Publisher Identifier",
            "default": "https://www.re3data.org/repository/r3d100013504",
            "$dataciteRequired": false
          },
          "publisherIdentifierScheme": {
            "type": "string",
            "title": "Publisher Identifier Scheme",
            "default": "r3data",
            "$dataciteRequired": false
          },
          "schemeUri": {
            "type": "string",
            "title": "Scheme URI",
            "default": "https://re3data.org/",
            "$dataciteRequired": false
          }
        },
        "required": [ "name", "publisherIdentifier", "publisherIdentifierScheme", "schemeUri" ],
        "$dataciteRequired": true
      },
      "publicationYear": {
        "type": "integer",
        "title": "Publication Year",
        "mininum": 2000,
        "maximum": 9999,
        "$dataciteRequired": true
      },
      "contributors": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "contributorType": {
              "type": "string",
              "title": "Contributor Type",
              "$dataciteRequired": true,
              "enum": [
                "ContactPerson",
                "DataCollector",
                "DataCurator",
                "DataManager",
                "Distributor",
                "Editor",
                "HostingInstitution",
                "Producer",
                "ProjectLeader",
                "ProjectManager",
                "RegistrationAgency",
                "RegistrationAuthority",
                "RelatedPerson",
                "Researcher",
                "RightsHolder",
                "Sponsor",
                "Supervisor",
                "Translator",
                "WorkPackageLeader",
                "Other"
              ]
            },
            "name": {
              "type": "string",
              "title": "Contributor Name",
              "minLength": 1,
              "$dataciteRequired": true
            },
            "nameType": {
              "type": "string",
              "title": "Name Type",
              "$dataciteRequired": false,
              "enum": [ "Personal" ],
              "default": "Personal"
            },
            "givenName": {
              "type": "string",
              "title": "Given Name",
              "minLength": 1,
              "$dataciteRequired": false
            },
            "familyName": {
              "type": "string",
              "title": "Family Name",
              "minLength": 1,
              "$dataciteRequired": false
            },
            "nameIdentifiers": {
              "type": "array",
              "title": "Name Identifiers",
              "items": {
                "type": "object",
                "properties": {
                  "nameIdentifier": {
                    "type": "string",
                    "title": "ORCID",
                    "pattern": "^https://orcid.org/(\\d{4}-){3}\\d{3}(\\d|X)$",
                    "$dataciteRequired": false
                  },
                  "nameIdentifierScheme": {
                    "type": "string",
                    "title": "Identifier Scheme",
                    "enum": [ "ORCID" ],
                    "default": "ORCID",
                    "$dataciteRequired": true
                  },
                  "schemeUri": {
                    "type": "string",
                    "title": "Scheme URI",
                    "enum": [ "https://orcid.org" ],
                    "default": "https://orcid.org",
                    "$dataciteRequired": false
                  }
                },
                "required": [ "nameIdentifier", "nameIdentifierScheme", "schemeUri" ]
              },
              "$dataciteRequired": false
            },
            "affiliation": {
              "type": "array",
              "title": "Affiliations",
              "minItems": 1,
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "title": "Affiliation Name",
                    "minLength": 1,
                    "$dataciteRequired": true
                  },
                  "affiliationIdentifier": {
                    "type": "string",
                    "pattern": "^https://ror.org/0[a-z|0-9]{6}[0-9]{2}$",
                    "title": "ROR URL",
                    "$dataciteRequired": false
                  },
                  "affiliationIdentifierScheme": {
                    "type": "string",
                    "title": "Affiliation Identifier Scheme",
                    "enum": [ "ROR" ],
                    "$dataciteRequired": true
                  },
                  "schemeUri": {
                    "type": "string",
                    "title": "Scheme URI",
                    "enum": [ "https://ror.org" ],
                    "$dataciteRequired": false
                  }
                },
                "required": [ "name" ],
                "dependentRequired": {
                  "affiliationIdentifier": [ "affiliationIdentifierScheme", "schemeUri" ]
                }
              },
              "$dataciteRequired": false
            }
          },
          "required": [ "contributorType", "name", "givenName", "familyName", "affiliation" ],
          "$dataciteRequired": false
        }
      },
      "subjects": {
        "type": "array",
