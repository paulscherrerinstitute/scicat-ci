            "schemeUri": {
              "type": "string",
              "title": "Scheme URI",
              "$dataciteRequired": false
            },
            "schemeType": {
              "type": "string",
              "title": "Scheme Type",
              "$dataciteRequired": false,
              "enum": [ "DOI", "Handle", "URL", "Other" ]
            }
          },
          "required": [
