            "dateInformation": {
              "type": "string",
              "title": "Date Information",
              "$dataciteRequired": false
            }
          },
          "required": [ "date", "dateType" ]
        },
        "$dataciteRequired": false
      },
      "language": {
        "type": "string",
        "title": "Language",
        "enum": [ "en" ],
        "default": "en",
        "$dataciteRequired": false
      },
      "types": {
