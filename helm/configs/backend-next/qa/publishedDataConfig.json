              "$dataciteRequired": true
            },
            "dateType": {
              "type": "string",
              "title": "Date Type",
              "$dataciteRequired": true,
              "enum": [
                "Accepted",
                "Available",
                "Copyrighted",
                "Collected",
                "Coverage",
                "Created",
                "Issued",
                "Submitted",
                "Updated",
                "Valid",
                "Withdrawn",
                "Other"
              ]
